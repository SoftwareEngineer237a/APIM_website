{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm",
    "analytics": "3c58e38d-19af-4c1b-bfbb-7787c6a0b68f"
  },
  "newProjectRoot": "projects",
  "projects": {
    "church-website": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              { "glob": "**/*", "input": "public", "output": "/" },
              { "glob": "**/*", "input": "src/assets", "output": "/assets" }
            ],
            "styles": ["src/styles.css"],
            "extractLicenses": false,
            "sourceMap": false,
            "optimization": true,
            "namedChunks": false,
            "budgets": [
              {
                "type": "initial",
                "maximumWarning": "2MB",
                "maximumError": "5MB"
              },
              {
                "type": "anyComponentStyle",
                "maximumWarning": "100kB",
                "maximumError": "200kB"
              }
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2MB",
                  "maximumError": "5MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "100kB",
                  "maximumError": "200kB"
                }
              ],
              "outputHashing": "all",
              "optimization": true
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": { "buildTarget": "church-website:build:production" },
            "development": { "buildTarget": "church-website:build:development" }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular/build:unit-test"
        }
      }
    }
  }
}